param set MC_ROLLRATE_MAX 1200.0
param set MC_ROLLRATE_P 0.050000000745058060
param set MC_ROLL_P 10.00
param set MC_YAW_P 2.800000190734863281
param set MPC_ACC_HOR_MAX 12.00
param set MPC_ACC_UP_MAX 12.0
param set MPC_ALT_MODE 2
param set MPC_POS_MODE 3
param set MPC_RPT_R_DRAG_X 0.100000001490116119
param set MPC_RPT_XY_EPS 0.300000011920928955
param set MPC_RPT_XY_KI 0.100000001490116119
param set MPC_RPT_XY_MAX_I 3.000
param set MPC_RPT_XY_SIGMA 0.800000011920928955
param set MPC_RPT_XY_WN 1.000
param set MPC_RPT_Z_KI 0.150000005960464478
param set MPC_RPT_Z_SIGMA 2.000
param set MPC_RPT_Z_WN 1.200000047683715820
param set MPC_THR_HOVER 0.280000001192092896
param set MPC_THR_XY_MARG 0.50
param set MPC_TILTMAX_AIR 89.0
param set MPC_USE_HTE 0
param set MPC_VEL_MANUAL 0.5
param set MPC_XY_CRUISE 3.0
param set MPC_XY_P 1.386800050735473633
